<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="treeviz.forestfirequiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="treeviz.forestfirequiz.MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="treeviz.forestfirequiz.PlayAgain" />
        <activity android:name="treeviz.forestfirequiz.WinActivity" />
        <activity android:name="treeviz.forestfirequiz.outOfTime" />
        <activity android:name="treeviz.forestfirequiz.GameActivity">

        </activity>
    </application>

</manifest>